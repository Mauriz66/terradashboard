{
  "version": 2,
  "buildCommand": "./vercel-build.sh",
  "outputDirectory": "dist",
  "framework": null,
  "routes": [
    { "src": "/api/health", "dest": "/api/health.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|webp|woff|woff2|ttf|json))", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 